_require "basis.smi"
structure Type =
struct
  datatype ty =
      INTty
    | STRINGty
    | BOOLty
    | TYVARty of string  (* 型変数 *)
    | FUNty of ty * ty
    | PAIRty of ty * ty
    | POLYty of string list * ty
  (* 呼ばれるごとに新しい型変数を返す関数 *)
  val newTy : unit -> ty
  val tyToString : ty -> string
end
